              "id": "org.ekstep.shape:create",
              "data": {
                "type": "polygon",
                "x": 20,
                "y": 20,
                "fill": "#FF0000",
                "w": 14,
                "h": 25,
                "sides": 5,
                "stroke": "rgba(255, 255, 255, 0)",
                "strokeWidth": 1,
                "opacity": 1
              }
            }
          },
          {
            "id": "trapezium",
            "type": "icon",
            "toolTip": "Add Trapezium",
            "title": "Trapezium",
            "iconClass": "road icon",
